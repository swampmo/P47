<?xml version="1.0"?>
<PropertyList>

 <path>water-pressure.ac</path>

 <animation>
  <type>material</type>
  <object-name>face</object-name>
  <object-name>needle</object-name>
  <object-name>ring</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
            <red>10</red>
            <green>0</green>
            <blue>8</blue>
    </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>engines/engine[0]/oil-temp-degc</property>
  <min>0.0</min>
  <max>200</max>
   <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>130</ind>
    <dep>83</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>-0.017</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
