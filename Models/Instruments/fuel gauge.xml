<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>fuel gauge.ac</path>

  <animation>
    <object-name>cover</object-name>
    <object-name>MainTankLever</object-name>
        <object-name>fuel gauge</object-name>
    <object-name>Bolts</object-name>
  </animation>

    <animation>
    <type>material</type>
    <object-name>MainTankLever</object-name>
    <object-name>cover</object-name>
    <emission>
      <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>10</red>
            <green>0</green>
            <blue>8</blue>
    </emission>
  </animation>

      <animation>
    <type>rotate</type>
    <object-name>MainTankLever</object-name>
    <property>consumables/fuel/tank</property>
    <factor>5</factor>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>1.0</z>
    </axis>
    <center>
     <x-m>-0.37090</x-m>
     <y-m>-0.37154</y-m>
     <z-m>0.13021</z-m>
   </center>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>MainTankLever</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-adjust</command>
        <property>consumables/fuel/tank</property>
        <step>-5</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>MainTankLever</object-name>
    <action>
      <button>1</button>
      <binding>
        <command>property-adjust</command>
        <property>consumables/fuel/tank</property>
        <step>5</step>
      </binding>
    </action>
  </animation>

</PropertyList>
