<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertyList>

    <path>primer.ac</path>

  <animation>
    <object-name>Egr.base</object-name>
    <object-name>Nut</object-name>
    <object-name>Pmr.base</object-name>
    <object-name>primer.switch</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Egr.base</object-name>
    <object-name>Nut</object-name>
    <object-name>Pmr.base</object-name>
    <object-name>primer.switch</object-name>
    <emission>
      <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>10</red>
            <green>0</green>
            <blue>8</blue>
    </emission>
  </animation>

    <animation>
        <type>rotate</type>
        <object-name>primer.switch</object-name>
        <property>controls/engines/engine/primer</property>
        <factor>-60</factor>
        <center>
            <x-m>0.002</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1.0</y>
            <z>0</z>
        </axis>
    </animation>

   <animation>
        <type>pick</type>
        <object-name>Pmr.base</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>controls/engines/engine/primer</property>
            </binding>
        </action>
    </animation>


</PropertyList>

