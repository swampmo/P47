<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>temp-oil-fuel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>visses</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <object-name>needle3</object-name>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/P47/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <object-name>needle3</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
            <red>10</red>
            <green>0</green>
            <blue>8</blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <!--property>engines/engine/oil-temp-deg-c</property-->
        <property>environment/temperature-degc</property>
    <!--interpolation>
      <entry><ind> -50 </ind><dep> -27 </dep></entry>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  50 </ind><dep>  33 </dep></entry>
      <entry><ind> 100 </ind><dep>  65 </dep></entry>
    </interpolation-->
    <interpolation>
      <!--entry><ind>-50 </ind><dep> 0 </dep></entry-->
      <entry><ind> 0 </ind><dep> 0</dep></entry>
      <entry><ind> 10 </ind><dep>5 </dep></entry>
      <entry><ind> 20 </ind><dep>15 </dep></entry>
      <entry><ind> 30 </ind><dep>25 </dep></entry>
      <entry><ind> 40 </ind><dep>35 </dep></entry>
      <entry><ind> 50 </ind><dep>45 </dep></entry>
      <entry><ind> 60 </ind><dep>55 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.00693 </x-m>
      <y-m> 0.00065 </y-m>
      <z-m> 0.00240 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle2</object-name>
      <property>engines/engine/oil-pressure-psi</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>10</ind><dep>5</dep></entry>
   <entry><ind>20</ind><dep>15</dep></entry>
   <entry><ind>30</ind><dep>25</dep></entry>
   <entry><ind>40</ind><dep>35</dep></entry>
   <entry><ind>50</ind><dep>45</dep></entry>
   <entry><ind>60</ind><dep>55</dep></entry>
   <entry><ind>70</ind><dep>65</dep></entry>
   <entry><ind>80</ind><dep>75</dep></entry>
   <entry><ind>90</ind><dep>85</dep></entry>
   <entry><ind>100</ind><dep>95</dep></entry>
  </interpolation>
    <center>
      <x-m>  0.00752 </x-m>
      <y-m> -0.00631 </y-m>
      <z-m> -0.01400 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle3</object-name>
    <property>engines/engine/fuel-flow-gph</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 35 </ind><dep> 180 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.00743 </x-m>
      <y-m>  0.00777 </y-m>
      <z-m> -0.01390 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
