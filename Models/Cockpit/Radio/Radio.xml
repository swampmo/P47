<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>Radio.ac</path>

  <animation>
    <object-name>Bolts</object-name>
    <object-name>Case</object-name>
    <object-name>Dial.1</object-name>
    <object-name>Dial.2</object-name>
    <object-name>FineTune.1</object-name>
    <object-name>FineTune.2</object-name>
    <object-name>Glass.1</object-name>
    <object-name>Glass.2</object-name>
    <object-name>Tuner.1</object-name>
    <object-name>Tuner.2</object-name>
 </animation>

   <!-- working for Rembrandt -->

  <effect>
    <inherits-from>Aircraft/P47/Models/Effects/Glass/glass</inherits-from>
    <object-name>Glass.1</object-name>
    <object-name>Glass.2</object-name>
  </effect>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>Glass.1</object-name>
    <object-name>Glass.2</object-name>
  </effect>

  <animation>
  <type>material</type>
    <object-name>Dial.1</object-name>
    <object-name>Dial.2</object-name>
    <object-name>FineTune.1</object-name>
    <object-name>FineTune.2</object-name>
    <object-name>Tuner.1</object-name>
    <object-name>Tuner.2</object-name>

    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
            <red>0.60</red>
            <green>0</green>
            <blue>0</blue>
    </emission>

  </animation>

</PropertyList>
