  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>prop.lever</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/propeller-pitch</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>prop.lever</object-name>
    <action>
      <button>1</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/propeller-pitch</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>boost.lever</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/wastegate</property>
        <step>0.1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>boost.lever</object-name>
    <action>
      <button>1</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/wastegate</property>
        <step>-0.1</step>
      </binding>
    </action>
  </animation>

    <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>water.inj.wheel</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/water-injection</property>
        <step>1</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>water.inj.wheel</object-name>
    <action>
      <button>1</button>
      <binding>
