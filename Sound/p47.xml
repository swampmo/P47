    <engine>
        <name>engine1_runninginside</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/doublewaspinside.wav</path>
        <condition>
            <property>engines/engine[0]/running</property>
        </condition>
        <volume>
            <property>engines/engine[0]/mp-inhg</property>
            <factor>0.026</factor>
            <offset>0.3</offset>
        </volume>
        <pitch>
            <property>engines/engine[0]/mp-inhg</property>
            <offset>0.0</offset>
            <factor>0.033</factor>
            <min>0.10</min>
            <max>1.2</max>
        </pitch>
        <position>
            <x>-3.10</x>
            <y>8.35</y>
            <z>-0.3</z>
        </position>
    </engine>


    <engine>
        <name>engine1_running</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/doublewasp.wav</path>
        <condition>
            <property>engines/engine[0]/running</property>
        <not-equals>
            <property>sim/current-view/internal</property>
                <value>1</value>
            </not-equals>
        </condition>
        <volume>
            <property>engines/engine[0]/mp-inhg</property>
            <factor>0.026</factor>
            <offset>0.2</offset>
        </volume>
        <pitch>
            <property>engines/engine[0]/mp-inhg</property>
            <offset>0.0</offset>
            <factor>0.033</factor>
            <min>0.10</min>
            <max>1.2</max>
        </pitch>
        <position>
            <x>-3.10</x>
            <y>8.35</y>
            <z>-0.3</z>
        </position>
        <reference-dist>150.0</reference-dist>
        <max-dist>5000.0</max-dist>
    </engine>

    <rumble>
        <name>rumble</name>
        <mode>looped</mode>
        <path>Sounds/rumble.wav</path>
        <condition>
            <or>
                <property>gear/gear[0]/wow</property>
                <property>gear/gear[1]/wow</property>
                <property>gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>velocities/groundspeed-kt</property>
            <factor>0.005</factor>
        </volume>
        <position>
            <x>0.0</x>
            <y>0</y>
            <z>-3</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </rumble>

    <gear>
        <name>gear</name>
        <mode>in-transit</mode>
        <path>Sounds/gear.wav</path>
        <property>gear/gear[0]/position-norm</property>
        <pitch>
            <internal>dt_play</internal>
            <offset>1.0</offset>
            <factor>-0.001</factor>
        </pitch>
        <position>
            <x>0</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear>

    <gear>
        <name>gear-lock</name>
        <path>Sounds/gear-lck.wav</path>
        <condition>
            <equals>
                <property>gear/gear[0]/position-norm</property>
                <value>0</value>
            </equals>
        </condition>
        <position>
            <x>0</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear>

    <squeal>
        <name>squeal</name>
        <path>Sounds/squeal.wav</path>
        <condition>
            <or>
                <property>gear/gear[0]/wow</property>
                <property>gear/gear[1]/wow</property>
                <property>gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>velocities/speed-down-fps</property>
            <factor>0.05</factor>
        </volume>
        <volume>
            <property>velocities/airspeed-kt</property>
            <factor>0.02</factor>
        </volume>
        <volume>
            <internal>dt_stop</internal>
            <factor>0.05</factor>
            <max>1.0</max>
        </volume>
        <pitch>
            <property>velocities/airspeed-kt</property>
            <factor>0.0025</factor>
            <offset>1.2</offset>
        </pitch>
        <position>
            <x>0.0</x>
            <y>0</y>
            <z>-2</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>50.0</max-dist>
    </squeal>

    <flap>
        <name>flaps</name>
        <mode>in-transit</mode>
        <path>Sounds/flaps.wav</path>
        <property>surface-positions/flap-pos-norm</property>
        <volume>
            <factor>0.5</factor>
        </volume>
        <position>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </flap>

    <guns>
        <name>guns</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/50cal.wav</path>
        <property>controls/armament/trigger-cannon</property>
        <volume>
            <property>controls/armament/trigger-cannon</property>
        </volume>
        <position>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>50.0</max-dist>
</guns>

    <rockets>
	    <name>rockets</name>
	    <mode>once</mode>
	    <!-- placeholder, should change it to a more realistic sound. :) -->
	    <path>Aircraft/P47/Sound/startup.wav</path>
	    <condition>
		    <property>controls/armament/rocket-sound-on</property>
	    </condition>
	    <volume>
		    <offset>1</offset>
	    </volume>
	    <position>
		    <x>0</x>
		    <y>0</y>
		    <z>-1</z>
	    </position>
	    <reference-dist>20.0</reference-dist>
	    <max-dist>50.0</max-dist>
    </rockets>


    <wind>
        <name>wind</name>
        <mode>looped</mode>
        <path>Sounds/wind.wav</path>
        <property>velocities/airspeed-kt</property>
        <volume>
            <property>velocities/airspeed-kt</property>
            <factor>0.0015</factor>
            <min>0.01</min>
            <max>0.15</max>
        </volume>
        <pitch>
            <property>velocities/airspeed-kt</property>
            <factor>0.01</factor>
            <offset>1.25</offset>
        </pitch>
        <position>
            <x>-0.5</x>
            <y>0</y>
            <z>1.25</z>
        </position>
        <reference-dist>1</reference-dist>
        <max-dist>2.0</max-dist>
    </wind>

    <switch>
        <name>click on</name>
        <mode>once</mode>
        <path>Sounds/click.wav</path>
        <condition>
            <property>sim/sound/click</property>
        </condition>
        <volume>
            <offset>0.5</offset>
        </volume>
        <position>
            <x>-0.6</x>
            <y>0</y>
            <z>0.25</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </switch>

    <switch>
        <name>click off</name>
        <mode>once</mode>
        <path>Sounds/click.wav</path>
        <condition>
            <not>
                <property>sim/sound/click</property>
            </not>
        </condition>
        <volume>
            <offset>0.5</offset>
        </volume>
        <position>
            <x>-0.6</x>
            <y>0</y>
            <z>0.25</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </switch>

        <aircraft-explode>
            <name>aircraft-explode</name>
            <path>Aircraft/P47/Sound/aircraft-explode.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/explode-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>3</factor>
                <max>3</max>
            </volume>
        </aircraft-explode>

        <aircraft-detach>
            <name>aircraft-detach</name>
            <path>Aircraft/P47/Sound/aircraft-wings-detach.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/detach-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>1</factor>
            </volume>
        </aircraft-detach>

        <aircraft-crash>
            <name>aircraft-impact</name>
            <path>Aircraft/P47/Sound/aircraft-crash.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/crash-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>1</factor>
            </volume>
        </aircraft-crash>

        <aircraft-water-crash>
            <name>aircraft-water-crash</name>
            <path>Aircraft/P47/Sound/aircraft-water-crash.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/water-crash-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>1</factor>
            </volume>
        </aircraft-water-crash>

        <aircraft-crack>
            <name>aircraft-crack</name>
            <path>Aircraft/P47/Sound/aircraft-crack.wav</path>
            <mode>once</mode>
            <condition>
                <property>sim/current-view/internal</property>
                <equals>
                    <property>damage/sounds/crack-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <property>damage/sounds/crack-volume</property>
                <factor>1</factor>
            </volume>
        </aircraft-crack>

        <aircraft-creaking>
            <name>aircraft-creaking</name>
            <path>Aircraft/P47/Sound/aircraft-creaking.wav</path>
            <mode>looped</mode>
            <condition>
                <property>sim/current-view/internal</property>
                <equals>
                    <property>damage/sounds/creaking-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <property>damage/sounds/creaking-volume</property>
                <factor>1</factor>
            </volume>
        </aircraft-creaking>



</fx>
</PropertyList>
