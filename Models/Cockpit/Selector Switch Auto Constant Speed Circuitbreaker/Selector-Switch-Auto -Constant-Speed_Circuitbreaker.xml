<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>Selector-Switch-Auto -Constant-Speed_Circuitbreaker.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>AutoProp Switch</object-name>
    <object-name>AutoPropControl</object-name>
    <object-name>Bolts</object-name>
    <object-name>DiveBrake</object-name>
    <object-name>DiveBrakeCover</object-name>
    <object-name>Glass</object-name>
    <object-name>RedLamp</object-name>
    <object-name>ReSetButton</object-name>
    <object-name>ReSetButtonFrame</object-name>
    <object-name>SwitchNutDiveBrake</object-name>
    <object-name>SwitchNutPropControl</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/P47/Models/Effects/Glass/glass</inherits-from>
    <object-name>Glass</object-name>
  </effect>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>Glass</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>AutoProp Switch</object-name>
    <object-name>DiveBrake</object-name>
    <object-name>DiveBrakeCover</object-name>
    <object-name>ReSetButton</object-name>
    <object-name>SwitchNutDiveBrake</object-name>
    <object-name>SwitchNutPropControl</object-name>

    <emission>
      <factor-prop>/controls/lighting/cabin-norm</factor-prop>
             <red>0</red>
            <green>1</green>
            <blue>8</blue>
    </emission>
  </animation>

</PropertyList>
