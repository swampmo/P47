<?xml version="1.0"?>
<PropertyList>

    <path>mp.ac</path>

    <animation>
        <type>material</type>
        <object-name>MP.face</object-name>
        <object-name>MP.needle1</object-name>
        <object-name>MP.needle2</object-name>
        <emission>
            <red>0.8</red>
            <green>0.5</green>
            <blue>0.3</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>MP.needle1</object-name>
        <property>engines/engine/mp-inhg</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>10</ind><dep>0.0</dep></entry>
            <entry><ind>75</ind><dep>350</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

</PropertyList>