<?xml version="1.0"?>
<PropertyList>

<fx>

<!-- Engine 1 -->
    <crank>
        <name>crank1</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/starter.wav</path>
        <condition>
            <property>engines/engine[0]/cranking</property>
            <not>
                <property>engines/engine[0]/running</property>
            </not>
        <not-equals>
            <property>sim/current-view/internal</property>
                <value>1</value>
        </not-equals>
        </condition>
        <volume>
            <property>engines/engine[0]/cranking</property>
        </volume>
        <position><!-- +X = back / +Y = left / +Z = up-->
            <x>3.5</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>50.0</max-dist>
    </crank>

    <crank>
        <name>crank1inside</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/starterinside.wav</path>
        <condition>
            <property>engines/engine[0]/cranking</property>
            <not>
                <property>engines/engine[0]/running</property>
            </not>
        </condition>
        <volume>
            <property>engines/engine[0]/cranking</property>
        </volume>
        <position><!-- +X = back / +Y = left / +Z = up-->
            <x>3.5</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>50.0</max-dist>
    </crank>

     <energizer>
    <name>energizerinside</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/energizerinside.wav</path>
               <property>engines/engine/energizer</property>
            <pitch>
            <property>engines/engine/energizer</property>
            <offset>0.5</offset>
            <factor>1.5</factor>
        </pitch>
        <position><!-- +X = back / +Y = left / +Z = up-->
            <x>3.5</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>2.0</reference-dist>
        <max-dist>100.0</max-dist>
    </energizer>

   <energizer>
    <name>energizer</name>
        <mode>looped</mode>
        <path>Aircraft/P47/Sound/energizer.wav</path>
        <condition>
                <property>engines/engine/energizer</property>
        <not-equals>
            <property>sim/current-view/internal</property>
                <value>1</value>
        </not-equals>
        </condition>
        <pitch>
            <property>engines/engine/energizer</property>
            <offset>0.5</offset>
            <factor>1.5</factor>
        </pitch>
        <position><!-- +X = back / +Y = left / +Z = up-->
            <x>3.5</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>2.0</reference-dist>
        <max-dist>100.0</max-dist>
    </energizer>

