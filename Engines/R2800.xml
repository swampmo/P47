<?xml version="1.0"?>

<!--
  File:     R2800.xml
  Author:   AeromatiC++ v 3.3.0
  See: http://wiki.flightgear.org/JSBSim_Engines#FGPiston
  Inputs:
    name:           R2800
    type:           Piston Engine
    power:          2600 hp
-->

<piston_engine name="R2800">
  <minmp unit="INHG">         6.8 </minmp>
  <maxmp unit="INHG">         45.5 </maxmp>
    <displacement unit="IN3"> 3442 </displacement>
 <cylinder-head-mass unit="LBS"> 25.0 </cylinder-head-mass>
  <maxhp>        2000 </maxhp>
  <cycles>         4.0 </cycles>
  <idlerpm>      400.0 </idlerpm>
  <maxrpm>      2700.0 </maxrpm>
  <sparkfaildrop>  0.9 </sparkfaildrop>
  <volumetric-efficiency> 0.63 </volumetric-efficiency>
  <man-press-lag> 0.1 </man-press-lag>
  <static-friction  unit="HP"> 19 </static-friction>
  <starter-torque> 2080 </starter-torque>
  <starter-rpm> 1000 </starter-rpm>
 <!-- Defining <bsfc> over-rides the built-in horsepower calculations -->
  <bsfc>           0.27 </bsfc>
  <stroke unit="IN">  6 </stroke>
  <bore unit="IN">    5.75 </bore>
  <cylinders>         18  </cylinders>
  <compression-ratio>  6.0 </compression-ratio>
  <cooling-factor> 3 </cooling-factor>
  <boostauto>      1 </boostauto>
  <numboostspeeds>   2 </numboostspeeds>
  <boostoverride>    0 </boostoverride>
  <takeoffboost>     0</takeoffboost> 


 <!-- Low-speed supercharger; see table above.  The rated boost is on top of
       sea level ambient pressure. -->
  <ratedboost1 unit="INHG"> 20.36 </ratedboost1>
  <ratedpower1> 2000 </ratedpower1>
  <ratedrpm1>   2700 </ratedrpm1>
  <ratedaltitude1> 10 </ratedaltitude1>

  <!-- High-speed supercharger; see table above. -->
  <ratedboost2 unit="INHG"> 26.46 </ratedboost2>
  <ratedpower2> 2000 </ratedpower2>
  <ratedrpm2>   2700 </ratedrpm2>
  <ratedaltitude2> 10000 </ratedaltitude2>


</piston_engine>