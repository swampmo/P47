<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>belly fuel.ac</path>

  <animation>
    <object-name>cover</object-name>
    <object-name>lever</object-name>
        <object-name>fuel.gauge</object-name>
    <object-name>Bolts</object-name>
  </animation>

    <animation>
    <type>rotate</type>
    <object-name>lever</object-name>
    <property>consumables/fuel/tank[1]</property>
    <factor>5</factor>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>1.0</z>
    </axis>
    <center>
     <x-m>-0.37116</x-m>
     <y-m>-0.37168</y-m>
     <z-m>0.13021</z-m>
   </center>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>lever</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-adjust</command>
        <property>consumables/fuel/tank[1]</property>
        <step>-5</step>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>lever</object-name>
    <action>
      <button>1</button>
      <binding>
        <command>property-adjust</command>
        <property>consumables/fuel/tank[1]</property>
        <step>5</step>
      </binding>
    </action>
  </animation>

</PropertyList>
