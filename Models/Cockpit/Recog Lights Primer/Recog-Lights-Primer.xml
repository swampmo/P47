<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>Recog-Lights-Primer.ac</path>

  <animation>
    <object-name>KI.LT</object-name>
    <object-name>KS.holder</object-name>
    <object-name>amber.light</object-name>
    <object-name>amber.switch</object-name>
    <object-name>black.lamp</object-name>
    <object-name>cowl.flaps</object-name>
    <object-name>face</object-name>
    <object-name>glass</object-name>
    <object-name>green.switch</object-name>
    <object-name>keying.switch</object-name>
    <object-name>primer.handle</object-name>
    <object-name>primer.panel</object-name>
    <object-name>red.switch</object-name>
    <object-name>switch.nut.amber</object-name>
    <object-name>switch.nut.green</object-name>
    <object-name>switch.nut.red</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>amber.switch</object-name>
    <object-name>green.switch</object-name>
    <object-name>keying.switch</object-name>
    <object-name>red.switch</object-name>
    <object-name>switch.nut.amber</object-name>
    <object-name>switch.nut.green</object-name>
    <object-name>switch.nut.red</object-name>
    <object-name>face</object-name>
    <emission>
      <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>0</red>
            <green>1</green>
            <blue>8</blue>
    </emission>
  </animation>
    <animation>
        <type>translate</type>
        <object-name>cowl.flaps</object-name>
        <property>/controls/engines/engine/cowl-flaps</property>
        <factor>10</factor>
        <center>
            <x-m>-0.71621</x-m>
            <y-m>0.40201</y-m>
            <z-m>0.35337</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
  </animation>

     <animation>
        <type>pick</type>
        <object-name>cowl.flaps</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>/controls/engines/engine/cowl-flaps-norm</property>
            </binding>

            <binding>
                <command>property-toggle</command>
                <property>sim/sound/click</property>
            </binding>
        </action>
    </animation>

</PropertyList>
