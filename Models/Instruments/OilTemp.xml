<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

 <path>OilTemp.ac</path>

   <animation>
  <type>material</type>
  <object-name>PointerTip</object-name>
  <object-name>Face</object-name>
     <emission>
      <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>10</red>
            <green>0</green>
            <blue>8</blue>
    </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>PointerTip</object-name>
  <property>fdm/jsbsim/propulsion/engine/oil-temperature-degF</property>
  <interpolation>
      <entry><ind> 100 </ind><dep>  0 </dep></entry>
      <entry><ind> 110 </ind><dep> 15 </dep></entry>
      <entry><ind> 120 </ind><dep> 25 </dep></entry>
      <entry><ind> 130 </ind><dep> 35 </dep></entry>
      <entry><ind> 140 </ind><dep> 48 </dep></entry>
      <entry><ind> 150 </ind><dep> 60 </dep></entry>
      <entry><ind> 160 </ind><dep> 70 </dep></entry>
      <entry><ind> 170 </ind><dep> 85 </dep></entry>
      <entry><ind> 180 </ind><dep> 95 </dep></entry>
      <entry><ind> 190 </ind><dep> 105 </dep></entry>
      <entry><ind> 200 </ind><dep> 125 </dep></entry>
      <entry><ind> 210 </ind><dep> 145 </dep></entry>
      <entry><ind> 220 </ind><dep> 155 </dep></entry>
      <entry><ind> 230 </ind><dep> 175 </dep></entry>
      <entry><ind> 240 </ind><dep> 195 </dep></entry>
      <entry><ind> 250 </ind><dep> 225 </dep></entry>
      <entry><ind> 260 </ind><dep> 235 </dep></entry>


  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>

</PropertyList>
