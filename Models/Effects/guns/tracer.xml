<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>tracer.ac</path>

  <animation>
    <type>dist-scale</type>
    <name>50 BMG tracer</name>
    <object-name>trace</object-name>
    <interpolation>
      <entry><ind>    0 </ind><dep>  0.05 </dep></entry>
      <entry><ind>   10 </ind><dep>  0.20 </dep></entry>
      <entry><ind>  400 </ind><dep> 10.00 </dep></entry>
      <entry><ind> 1000 </ind><dep>  0.00 </dep></entry>
    </interpolation>
  </animation>

</PropertyList>
