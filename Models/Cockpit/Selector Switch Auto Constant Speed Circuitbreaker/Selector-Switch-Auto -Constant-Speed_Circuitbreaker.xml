  <path>Selector-Switch-Auto -Constant-Speed_Circuitbreaker.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>AutoProp Switch</object-name>
    <object-name>AutoPropControl</object-name>
    <object-name>Bolts</object-name>
    <object-name>DiveBrake</object-name>
    <object-name>DiveBrakeCover</object-name>
    <object-name>Glass</object-name>
