<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>suction.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>face</object-name>
    <object-name>frame</object-name>
    <object-name>needle</object-name>
    <object-name>Glass</object-name>

    <!-- Objets transparents -->
    <object-name>Glass</object-name>
  </animation>

  <!-- working for Rembrandt -->
    <effect>
    <inherits-from>Aircraft/P47/Models/Effects/Glass/glass</inherits-from>
    <object-name>Glass</object-name>
  </effect>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>glass</object-name>
  </effect>


  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>0</red>
            <green>1</green>
            <blue>8</blue>
    </emission>
  </animation>

  <animation>
	<type>rotate</type>
	<object-name>needle</object-name>
	<property>/systems/vacuum/suction-inhg</property>
	  <min>0</min>
  <max>10</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>10</ind>
    <dep>300</dep>
   </entry>
  </interpolation>
	<!--interpolation>
		<entry><ind> 0 </ind><dep> -18 </dep></entry>
		<entry><ind> 0.1 </ind><dep> 30 </dep></entry>
		<entry><ind> 3 </ind><dep> 35 </dep></entry>
		<entry><ind> 4 </ind><dep> 59 </dep></entry>
		<entry><ind> 5 </ind><dep> 83 </dep></entry>
		<entry><ind> 6 </ind><dep> 109 </dep></entry>
		<entry><ind> 99 </ind><dep> 115 </dep></entry>
	</interpolation-->
	  <center>
   <x-m>0.00182</x-m>
   <y-m>0.000008</y-m>
   <z-m>-0.00008</z-m>
  </center>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
</animation>

</PropertyList>
