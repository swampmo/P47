<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

 <path>OilPress.ac</path>

 <animation>
  <type>material</type>
  <object-name>PointerTip</object-name>
  <emission>
            <red>0.60</red>
            <green>0</green>
            <blue>0</blue>
   <factor-prop>/controls/lighting/cabin-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>PointerTip</object-name>
  <property>/engines/engine/oil-pressure-psi</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>10</ind><dep>0</dep></entry>
   <entry><ind>40</ind><dep>-56</dep></entry>
   <entry><ind>80</ind><dep>-113</dep></entry>
   <entry><ind>200</ind><dep>-280</dep></entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>

</PropertyList>
