<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

<path>turn.ac</path>

    <animation>
        <type>material</type>
        <object-name>TURN.face</object-name>
        <object-name>TURN.needle</object-name>
        <emission>
            <red>0.8</red>
            <green>0.5</green>
            <blue>0.3</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>TURN.needle</object-name>
        <property>instrumentation/turn-indicator/indicated-turn-rate</property>
        <factor>20</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Slip.ball</object-name>
        <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
        <interpolation>
            <entry><ind>-10.0</ind><dep>-20</dep></entry>
            <entry><ind>-2.0</ind><dep>-20</dep></entry>
            <entry><ind>2.0</ind><dep>20</dep></entry>
            <entry><ind>10.0</ind><dep>20</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0.118</z-m>
        </center>
    </animation>

</PropertyList>
