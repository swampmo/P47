<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>Oxygen Gauges.ac</path>
    <animation>
    <object-name>Flow.Gauge</object-name>
    <object-name>oxy.mount</object-name>
    <object-name>flow.gauge</object-name>
    <object-name>bolt</object-name>
    <object-name>vitre</object-name>
    <object-name>f.g.bezel</object-name>
  </animation>
    <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/P47/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>flow.gauge</object-name>
    <!--object-name>needle</object-name-->
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.08 </red>
      <green> 0.05 </green>
      <blue>  0.03 </blue>
    </emission>
  </animation>

</PropertyList>
