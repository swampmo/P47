<?xml version="1.0"?>

<!--
  File:     R-2800-59.xml
  Author:   AeromatiC++ v 3.3.10

  See: http://wiki.flightgear.org/JSBSim_Engines#FGPiston

  Inputs:
    name:           R-2800-59
    type:           Piston Engine
    power:          2535 hp
-->

<piston_engine name="R-2800-59">
  <minmp unit="INHG">          6.0 </minmp>
  <maxmp unit="INHG">         55.5 </maxmp>
    <displacement unit="IN3"> 2800 </displacement>
  <maxhp>        2535 </maxhp>
  <cycles>         4.0 </cycles>
  <idlerpm>      400.0 </idlerpm>
  <maxrpm>      2700.0 </maxrpm>
  <sparkfaildrop>  0.5 </sparkfaildrop>
  <volumetric-efficiency> 0.65 </volumetric-efficiency>
  <man-press-lag> 0.1 </man-press-lag>
  <static-friction  unit="HP"> 12.675 </static-friction>
  <starter-torque> 2800 </starter-torque>
  <starter-rpm> 1400 </starter-rpm>
 <!-- Defining <bsfc> over-rides the built-in horsepower calculations -->
 <bsfc>           0.27 </bsfc>
  <stroke unit="IN">  6 </stroke>
  <bore unit="IN">    5.8 </bore>
  <cylinders>         18  </cylinders>
  <compression-ratio>  6.75 </compression-ratio>
  <boostauto>      1 </boostauto>
  <numboostspeeds>   2 </numboostspeeds>
  <boostoverride>    0 </boostoverride>
  <takeoffboost>     1</takeoffboost> 


 <!-- Low-speed supercharger; see table above.  The rated boost is on top of
       sea level ambient pressure. -->
  <ratedboost1 unit="INHG"> 20.36 </ratedboost1>
  <ratedpower1> 2000 </ratedpower1>
  <ratedrpm1>   2700 </ratedrpm1>
  <ratedaltitude1> 0 </ratedaltitude1>

  <!-- High-speed supercharger; see table above. -->
  <ratedboost2 unit="INHG"> 26.46 </ratedboost2>
  <ratedpower2> 2000 </ratedpower2>
  <ratedrpm2>   2700 </ratedrpm2>
  <ratedaltitude2> 15000 </ratedaltitude2>

</piston_engine>
